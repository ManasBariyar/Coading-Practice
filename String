#include<iostream>
#include<string>
#include<vector>
#include<algorithm>

using namespace std;

bool anagrm(string str1, string str2, int n1, int n2){
    
    sort(str1.begin(), str1.end());
    sort(str2.begin(), str2.end());
    

    
    for(int i=0;i<str1.size();i++){
        
            if(str1[i] != str2[i]){
                return false;
            }
        
    }
    return true;
}

int main(){
    
    string str1 = "anagram";
    int n1 = str1.size();
    string str2 = "nagaram";
    int n2 = str2.size();
    
    
    cout<<str1<<endl;
    cout<<str2<<endl;
    
    bool ans = anagrm(str1,str2,n1,n2);
    
    if(ans){
        cout<<"Anagram"<<endl;
    }
    else{
        cout<<"Not an anagram"<<endl;
    }
    
    
    return 0;
}
